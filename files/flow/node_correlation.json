{
  "Name": "node_correlation",
  "ClassName": "org.qbroker.node.EventCorrelator",
  "URI": "event://localhost",
  "Operation": "correlate",
  "Capacity": "1024",
  "LinkName": "bymin",
  "SessionSize": "100",
  "SessionTimeout": "120",
  "XAMode": "1",
  "DisplayMask": "6",
  "Debug": "1",
  "StringProperty": {
    "hostname": "",
    "name": "",
    "priority": ""
  },
  "Ruleset": [
    {
      "Name": "shortcut",
      "PreferredOutLink": "action",
      "EventPattern": [
        {
          "priority": "^DEBUG$"
        }
      ]
    }
  ],
  "OutLink": [
    "action",
    "action",
    "action",
    "action"
  ]
}
